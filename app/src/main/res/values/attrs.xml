<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="backgroundColor" format="color"></attr>
    <attr name="foregroundColor" format="color" />
    <attr name="orientation">
        <enum name="horizontal" value = "1"></enum>
        <enum name="vertical" value = "2"></enum>
        <enum name="auto" value = "3"></enum>
    </attr>
    <declare-styleable name="ProgressBar">
        <attr name="backgroundColor"/>
        <attr name="foregroundColor"/>
        <attr name="orientation"/>
    </declare-styleable>
</resources>
